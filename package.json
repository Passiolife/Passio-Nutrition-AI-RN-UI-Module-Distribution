{
  "name": "@passiolife/nutrition-ai-ui-ux",
  "version": "3.2.0",
  "description": "A white-labeled nutrition tracking module implemented in React Native",
  "license": "MIT",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "source": "src/index",
  "react-native": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-nutrition-ux.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "yarn prepare && jest",
    "prepare": "bob build",
    "snapshot-update": "yarn prepare && jest -u",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "android": "cd example && yarn android",
    "ios": "cd example && yarn ios",
    "reverse": "adb reverse tcp:8081 tcp:8081"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "passio",
    "passiolife",
    "nutrition",
    "food",
    "nutritionai",
    "food scanner",
    "nutrition information",
    "ai advisor",
    "health"
  ],
  "repository": {
    "url": "git+https://github.com/Passiolife/Passio-Nutrition-AI-RN-UI-Module-Distribution.git"
  },
  "author": "Rami, Chirag (https://www.passiolife.com)",
  "bugs": {
    "url": "https://github.com/Passiolife/Passio-Nutrition-AI-RN-UI-Module-Distribution/issues"
  },
  "homepage": "https://github.com/Passiolife/Passio-Nutrition-AI-RN-UI-Module-Distribution#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/passiolife"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/Passiolife/Passio-Nutrition-AI-RN-UI-Module-Distribution/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "@babel/runtime": "^7.23.8",
    "@gorhom/bottom-sheet": "^4.6.1",
    "@notifee/react-native": "^7.8.2",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/datetimepicker": "^7.6.3",
    "@react-native-community/slider": "^4.5.0",
    "@react-native-picker/picker": "^2.7.7",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "intl": "^1.2.5",
    "jsc-android": "^250231.0.0",
    "lottie-ios": "4.3.4",
    "lottie-react-native": "6.4.1",
    "luxon": "^3.4.4",
    "patch-package": "^8.0.0",
    "react-error-boundary": "^4.0.12",
    "react-native-blob-util": "^0.19.6",
    "react-native-camera": "^4.2.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.16.0",
    "react-native-image-picker": "^7.1.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-markdown-display": "^7.0.2",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-pdf": "^6.7.4",
    "react-native-reanimated": "3.15.3",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.30.1",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^12.4.4",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-toast-message": "^2.2.0",
    "react-native-uuid": "^2.0.1",
    "use-async-resource": "^2.2.2",
    "victory-native": "^37.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@passiolife/nutritionai-react-native-sdk-v3": "3.2.1",
    "@react-native/eslint-config": "^0.73.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.0",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18.2.0",
    "@types/react-native": "0.72.8",
    "@types/react-native-sqlite-storage": "^6.0.5",
    "@types/react-test-renderer": "^18.2.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ft-flow": "^3.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "pod-install": "^0.1.39",
    "prettier": "^3.1.1",
    "react": "18.2.0",
    "react-native": "0.69.11",
    "react-native-builder-bob": "^0.20.0",
    "react-test-renderer": "^18.2.0",
    "release-it": "^17.0.1",
    "typescript": "5.0.4"
  },
  "resolutions": {
    "@types/react": "^18.2.0",
    "react-native": "0.69.11"
  },
  "peerDependencies": {
    "@passiolife/nutritionai-react-native-sdk-v3": "*",
    "react": "18.2.0",
    "react-native": "0.69.11"
  },
  "jest": {
    "globalSetup": "./global-setup.js",
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/",
      "<rootDir>/src/__tests__/utils/",
      "<rootDir>/src/__tests__/provider",
      "<rootDir>/src/__tests__/models",
      "<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic|@'@react-navigation|@react-navigation/native|@react-navigation/stack|react-native-sliders|react-native-screens|react-native-community)"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "react-native/no-inline-styles": 0,
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ],
      "no-restricted-imports": [
        "error",
        "@passiolife/nutrition-ai-ui-ux"
      ],
      "no-console": [
        "error"
      ],
      "no-shadow": "off"
    },
    "overrides": [
      {
        "files": [
          "example/**/*",
          "src/__tests__/**/*"
        ],
        "rules": {
          "no-restricted-imports": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
